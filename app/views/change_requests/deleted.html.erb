
	 	<div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">Deleted Incident report</h1>
        <% if flash[:notice] != nil %>
        <div class='alert alert-success' ><%= flash[:notice] %> </div>
        <% end %>
      </div>
      <br>
	<div class="row">
    <div class="col-lg-8">
      <div class="panel panel-default">
        <div class="panel-heading">Incident reports </div>
          <div class="panel-body">
            <table class='table table-striped'>
              <thead>
                <tr>
                  <th>Report ID</th>
                  <th>Service impacted</th>
                  <th>Problem details</th>
                  
                  <th colspan="3"></th>
                </tr>
              </thead>
              <tbody>
              	<% @change_requests.each do |document_version| %>
              	 <% change_request = document_version.reify %>
                  <tr>
                    <td><%= change_request.id %></td>
                    <td><%= change_request.priority %></td>
                    <td><%= change_request.category %></td>
                    
                   
                    <td><%= change_request.updated_at.to_s(:long) %></td>
                   <td><%= link_to 'Bringback', bringback_cr_version_path(document_version), method: 'PATCH' %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
            <%= paginate @change_requests, :theme => 'twitter-bootstrap-3' %>
        </div>
    </div>
